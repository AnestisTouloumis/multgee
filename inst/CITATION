citHeader("To cite multgee in publications use:")

citEntry(entry = "Article",
  title        = "{R} Package {multgee}: A Generalized Estimating Equations Solver for Multinomial Responses",
  author       = personList(as.person("Anestis Touloumis")),
  journal      = "Journal of Statistical Software",
  year         = "2015",
  volume       = "64",
  number       = "8",
  pages        = "1--14",
  url          = "https://www.jstatsoft.org/v64/i08/",

  textVersion  =
  paste("Anestis Touloumis (2015).",
        "R Package multgee: A Generalized Estimating Equations Solver for Multinomial Responses.",
        "Journal of Statistical Software, 64(8), 1-14.",
        "URL https://www.jstatsoft.org/v64/i08/.")
)

citEntry( header="To cite the methodology implemented in multgee in publications use:",
entry = "Article",
  title        = "GEE for multinomial responses using a local odds ratios parameterization",
  author       = personList(as.person("Anestis Touloumis"),as.person("Alan Agresti"),as.person("Maria Kateri")),
  journal      = "Biometrics",
  year         = "2013",
  volume       = "69",
  number       = "3",
  pages        = "633--640",
  url          = "https://onlinelibrary.wiley.com/enhanced/doi/10.1111/biom.12054/",

  textVersion  =
  paste("Anestis Touloumis, Alan Agresti and Maria Kateri (2013).",
        "R Package multgee: A Generalized Estimating Equations Solver for Multinomial Responses.",
        "Biometrics, 69(3), 633-640.",
        "URL https://onlinelibrary.wiley.com/enhanced/doi/10.1111/biom.12054/.")
)

